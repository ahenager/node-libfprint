{
  "name": "node-libfprint",
  "description": "libfprint bindings for node",
  "version": "0.0.1",
  "author": "Michael Wei <mwei@cs.ucsd.edu>",
  "main": "build/libfprint.js",
  "binary": {
    "module_name": "fprint",
    "module_path": "./build/bin/{module_name}/v{version}/{configuration}/{node_abi}-{platform}-{arch}",
    "remote_path": "./{module_name}/v{version}/{configuration}",
    "package_name": "{node_abi}-{platform}-{arch}.tar.gz",
    "host": ""
  },
  "private": "true",
  "dependencies": {
    "async": "^0.9.0",
    "bindings": "^1.2.1",
    "bluebird": "^2.3.6",
    "bunyan": "^1.0.1",
    "nan": "^1.3.0",
    "node-pre-gyp": "^0.5.27",
    "sprintf-js": "0.0.7"
  },
  "devDependencies": {
    "del": "^0.1.3",
    "gulp": "^3.8.8",
    "gulp-insert": "^0.4.0",
    "gulp-sourcemaps": "^1.2.2",
    "gulp-tsd": "0.0.3",
    "gulp-typescript": "^2.0.0"
  },
  "files": [
    "build"
  ],
  "bin": {
    "lsfprint": "./build/lsfprint.js"
  },
  "scripts": {
    "install": "node-pre-gyp install --fallback-to-build"
  }
}
